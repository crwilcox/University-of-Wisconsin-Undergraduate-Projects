void main() {
  int x;
  (x = 1); /* int */
  (x--); /* int */
  (x++); /* int */
  (++x); /* int */
  (--x); /* int */
  double y;
  (y = 1.0); /* double */
  (y--); /* double */
  (y++); /* double */
  (++y); /* double */
  (--y); /* double */
  int* z;
  (z++); /* int* */
  (z--); /* int* */
  (++z); /* int* */
  (--z); /* int* */
  return;
}
